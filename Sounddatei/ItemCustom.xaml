<TreeViewItem x:Class="Sounddatei.ItemCustom"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Sounddatei"
        xmlns:self="clr-namespace:Sounddatei"
        mc:Ignorable="d"
        ItemsSource="{Binding list}">
    <TreeViewItem.Header>
        <Grid>
            <Grid.ColumnDefinitions>
                <ColumnDefinition/>
                <ColumnDefinition/>
            </Grid.ColumnDefinitions>

            <Image Grid.Column="0"   Source="{Binding ImageBitMap}"/>
            <TextBlock Grid.Column="1" Text="{Binding FileName}"/>
        </Grid>
    </TreeViewItem.Header>
</TreeViewItem>
